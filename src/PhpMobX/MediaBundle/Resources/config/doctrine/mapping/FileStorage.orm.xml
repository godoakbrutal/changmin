<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">

    <entity name="PhpMob\MediaBundle\Entity\FileStorage" table="phpmob_media">
        <id name="id" type="string">
            <generator strategy="NONE"/>
        </id>

        <field name="text" type="blob" nullable="true"/>
        <field name="checksum" type="string" nullable="true"/>
        <field name="date" type="bigint" nullable="true"/>
    </entity>

</doctrine-mapping>
